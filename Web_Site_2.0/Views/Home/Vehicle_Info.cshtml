
@model Web_Site_2._0.Models.Vehicle_Info
@{

    Layout = "~/Views/Shared/_LayoutFourth.cshtml";
}

<!-- ======= Student Section ======= -->
<div class="container mt-5">
    <div class="row d-flex justify-content-center">
        <div class="col-md-7">
            <div class="card p-2 text-center">
                <div class="row">
                    <div class=" border-right no-gutters">
                        <div class="py-3">
                            <h2>Vehicle Information</h2>
                            <img src="data:image/jpeg;base64," width="100" class="rounded-circle">
                            <h4 class="text-secondary">Van ID = @Html.DisplayFor(model => model.Van_Id)</h4>

                            <div class="stats">
                                <table class="table table-borderless">
                                    <tbody>
                                        <tr>
                                            <td>
                                                <div class="d-flex flex-column"> <span class="text-left head">Van Liscense</span> <span class="text-left bottom">@Html.DisplayFor(model => model.Van_Licence)</span> </div>
                                            </td>
                                            
                                        </tr>
                                        <tr>
                                            <td>
                                                <div class="d-flex flex-column"> <span class="text-left head">Capacity</span> <span class="text-left bottom">@Html.DisplayFor(model => model.Capacity)</span> </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="px-3"><button type="button" onclick="window.location= '@Url.Action( "Edit", "Student")'" class="btn btn-primary btn-block">Edit</button></div>
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
</div>



